begin MeshGenerator

	type StructuredMeshGenerator

	dimension 3

	begin Geometry

		X0	0.0 
		Y0	0.0 
		Z0	0.0 

		array Nx
			2 1 2
		end

		array Dx
			3.0 2.0 3.0
		end

		array Ny
			1
		end

		array Dy
			1.0
		end

		array Nz
			1 1 1
		end

		array Dz
			-7.0 -4.0 -7.0
		end

		RegionMappingType uniform

		begin StructuredTransform
			type RotateYTransZTransform
			
			Theta  0.5236
			
			Dz     0.0
		end
	end

	begin RockRegionMap
		array Rock
			1
		end
	end

	begin RockData

		type Global

		array poro
			0.2
		end

		array permx 
			1e-15
		end

		array permy
			1e-15
		end

		array permz 
			1e-16
		end

	end
end
